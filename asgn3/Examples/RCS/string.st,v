1.5
log
@-
@
text
@#!/afs/cats.ucsc.edu/courses/cmps112-wm/usr/smalltalk/bin/gst -f
"$Id: string.st,v 1.4 2015-11-13 14:36:20-08 - - $"

nl := Character nl.

s := 'Hello world'.
stdout << s << nl.

(1 to: s size) do: [:i|
   |c ci|
   ci := s byteAt: i.
   cc := ci asCharacter.
   stdout << i << ' ' << ci << '=' << cc << ';' << nl.
   (cc = $o) ifTrue: [ stdout << 'True' << nl. ].
]

@


